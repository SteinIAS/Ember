<mjml>
   <mj-head>
      <mj-title>
         Ember template built using MJML
      </mj-title>
      <!-- breakpoint min-width:650 -->
      <mj-breakpoint width="650px" />
      <mj-attributes>
         <!-- Specificity order: Default MJML styles < default mj-attributes < classes < inline attribues  -->
         <mj-class name="small-font" font-size="12px" />
         <mj-all font-family="Arial, Helvetica, Sans-serif" font-size="16px" line-height="1.2" color="#666666" padding="0" />
         <mj-section background-color="#ffffff" />
         <mj-navbar-link text-transform="none" text-decoration="underline" color="blue" />
         <mj-social-element padding="0 10px 0 0" />
      </mj-attributes>

      <mj-style>
         /* Custom styles overwriting all styles */
         @media (min-width: 650px) {

            .left-align div,
            .left-align {
               text-align: left !important;
            }

            .right-align div,
            .right-align {
               text-align: right !important;
            }
         }

         /* Since outlook can't read media queries, the above styles have to be written again with outlook specific class */
         /* Outlook specific styles "-outlook" is added automatcially to classnames on mj-section when it is rendered to html */
         .desktop-align-outlook .left-align div,
         .desktop-align-outlook .left-align {
            text-align: left !important;
         }

         .desktop-align-outlook .right-align div,
         .desktop-align-outlook .right-align {
            text-align: right !important;
         }
      </mj-style>
   </mj-head>

   <mj-body background-color="#e7e7e7" width="650px">
      <mj-section css-class="desktop-align" padding="15px 25px">
         <mj-column padding="5px 0">
            <mj-text css-class="left-align" mj-class="small-font" align="center">
               Insert Johnson Line here
            </mj-text>
         </mj-column>
         <mj-column padding="5px 0">
            <mj-text css-class="right-align" mj-class="small-font" align="center">
               View in browser
            </mj-text>
         </mj-column>
      </mj-section>

      <mj-section padding="15px 25px" border-top="1px solid #cccccc;">
         <mj-group>
            <mj-column>
               <mj-image align="left" src="http://via.placeholder.com/240x120/438db9/ffffff" alt="" width="120px" />
            </mj-column>
            <mj-column>
               <mj-image align="right" src="http://via.placeholder.com/240x120/dd3977/ffffff" alt="" width="120px" />
            </mj-column>
         </mj-group>
      </mj-section>

      <mj-section>
