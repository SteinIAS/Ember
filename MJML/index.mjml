      <mj-section css-class="desktop-align" padding="0 25px">
         <mj-column width="250px" padding-bottom="20px">
            <mj-social icon-size="30px" css-class="left-align">
               <mj-social-element name="facebook-noshare" href="https://facebook.com"></mj-social-element>
